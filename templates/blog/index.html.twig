{% extends 'base.html.twig' %}


{% block title %}  Index {% endblock %}
{% block body %}
    {% for id,post in posts %}
        <p><a href="{{ path('blog_show',{'id':id }) }}">ID: {{ id }}</a></p>
        <h3>Title: {{ post.title }}</h3>
        <p>{{ post.text }}</p>


    {% endfor %}
{% endblock %}